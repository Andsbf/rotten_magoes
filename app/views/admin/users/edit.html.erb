<div style='text-center'>
  <%= link_to admin_users_path,:class=>"btn btn-info" do %>
  <i class="glyphicon glyphicon-chevron-left"></i> Back to All Users
<% end %>
</div>
<br>
<h1 style="text-align:center">Edit User</h1>
<%= render 'admin/users/form' %>

<div class='text-center'>
<%= link_to admin_user_path(@user.id),method: :delete ,data: { confirm: 'Are you sure you want to delete?' },:class=>"btn btn-danger"  do %>
  <i class="glyphicon glyphicon-remove"></i> Delete User
<% end %>
</div>
